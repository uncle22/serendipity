{
	"engine": "node",
	"services": [{
		"name": "com.palm.service.contacts",
		"assistant": "ContactsServiceAssistant",
		"description": "Service for various contacts-related methods",
		"globalized": true,
		"commands": [{
				"name": "preBackup",
				"assistant": "PhotoPreBackupAssistant"
			}, {
				"name": "countVCardContacts",
				"assistant": "VCardCountContactsAssistant"
			}, {
				"name": "readVCard",
				"assistant": "VCardReadAssistant",
